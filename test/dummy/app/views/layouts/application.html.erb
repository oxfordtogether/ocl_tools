<!DOCTYPE html>
<html>
  <head>
    <title>Dummy</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= action_cable_meta_tag %>
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>
  <body class="h-screen">
    <div class="min-h-screen bg-white">
      <nav class="bg-white">
        <div class="max-w-7xl mx-auto">
          <div class="flex justify-between h-16">
            <div class="flex">
              <div class="">
                <%= nav_tabs do |nav| %>
                  <% nav.link "Breadcrumbs", '/components/breadcrumbs' %>
                  <% nav.link "Button link to", '/components/button_link_to' %>
                  <% nav.link "Footer", '/components/footer' %>
                  <% nav.link "Form", '/people/new' %>
                  <% nav.link "Notice", '/components/notice' %>
                  <% nav.link "Pagination", '/components/pagination' %>
                  <% nav.link "Table", '/components/table' %>
                <% end %>
              </div>
            </div>
          </div>
        </div>
      </nav>
      <div class="py-10">
        <main>
          <div class="max-w-7xl mx-auto sm:px-6 lg:px-8">
            <%= yield %>
          </div>
        </main>
      </div>
    </div>
  </body>
</html>
